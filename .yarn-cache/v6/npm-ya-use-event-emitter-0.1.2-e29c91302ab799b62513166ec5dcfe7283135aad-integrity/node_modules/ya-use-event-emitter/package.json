{
  "name": "ya-use-event-emitter",
  "version": "0.1.2",
  "type": "module",
  "description": "React hook for subscribing to EventEmitter events with environment-aware cleanup",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    }
  },
  "files": [
    "src/**"
  ],
  "scripts": {
    "eslint": "eslint --no-error-on-unmatched-pattern \"src/**/*.js\" \"spec/**/*.js\"",
    "lint": "eslint --no-error-on-unmatched-pattern \"src/**/*.js\" \"spec/**/*.js\"",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "release:patch": "node scripts/release-patch.js",
    "test": "jasmine"
  },
  "keywords": [
    "events",
    "hook",
    "react",
    "emitter"
  ],
  "author": "kaspernj <kasper@diestoeckels.de> (github.com/kaspernj)",
  "license": "MIT",
  "dependencies": {
    "env-sense": ">= 1.0.2"
  },
  "peerDependencies": {
    "react": ">=18.3.0"
  },
  "devDependencies": {
    "@types/jasmine": "^5.1.4",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.9",
    "@types/react-test-renderer": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jsdoc": "^48.11.0",
    "jasmine": "^5.4.0",
    "react": "^18.3.1",
    "react-test-renderer": "^18.3.1",
    "typescript": "^5.6.2"
  }
}

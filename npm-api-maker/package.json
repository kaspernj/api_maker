    "ya-use-event-emitter": "^0.1.2",
    "ya-use-event-listener": ">= 0.1.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.26.3",
    "@jest/globals": "~30.2.0",
    "@types/react": "~19.0.10",
    "babel-jest": "~30.2.0",
